pipeline {
    agent any

    stages {

        stage('Checkout code') {
            steps {
                git branch: 'main', url: 'https://github.com/dor-amar/jenkins'
            }
        }
        stage('Build Docker Image') {
            steps{
                sh'''
                cd project
                docker build -t doramar97/myapp .
                '''
            }

        }


    }

}